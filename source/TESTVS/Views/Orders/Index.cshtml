@model IEnumerable<TESTVS.Models.Order>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Back to Goods List", "Index", "Goods")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Agent.AgentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Good.GoodsName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Good.GoodsUnit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.chargeState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.deliveryState)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Agent.AgentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Good.GoodsName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Good.GoodsUnit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.chargeState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.deliveryState)
        </td>
    </tr>
}

</table>
